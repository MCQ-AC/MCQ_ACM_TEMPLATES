//洛谷p3386
#include <bits/stdc++.h>
#define N 1001
using namespace std;
int n, m, e, ans, match[N];
bool a[N][N], vis[N];
bool dfs(int x)
{
    for (int i = 1; i <= m; i++)
        if (!vis[i] && a[x][i])
        {
            vis[i] = 1;
            if (!match[i] || dfs(match[i]))
            {
                match[i] = x;
                return 1;
            }
        }
    return 0;
}
int main()
{
    cin >> n >> m >> e; //n,m为二分图的两种节点数
    for (int i = 1; i <= e; i++)
    {
        int u, v;
        scanf("%d%d", &u, &v);
        if (v <= m) a[u][v] = 1;
    }
    for (int i = 1; i <= n; i++)
    {
        ans += dfs(i);
        memset(vis, 0, sizeof(vis));
    }
    cout << ans<<endl;
    return 0;
}
